<?xml version="1.0"?>

<DataProviders
  xmlns="xervice:dataprovider-01"
  xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
  xsi:schemaLocation="xervice:dataprovider-01 http://static.xervice.online/schema/dataprovider.schema.xsd"
>
    <DataProvider name="Credentials">
        <DataElement name="Email" type="string"/>
        <DataElement name="Password" type="string"/>
    </DataProvider>

    <DataProvider name="User">
        <DataElement name="UserId" type="int"/>
        <DataElement name="Email" type="string"/>
        <DataElement name="Password" type="string"/>
        <DataElement name="Firstname" type="string"/>
        <DataElement name="Lastname" type="string"/>
        <DataElement name="Company" type="string"/>
        <DataElement name="Token" type="string"/>
    </DataProvider>

</DataProviders>